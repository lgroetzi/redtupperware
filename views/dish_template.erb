<script type="text/template" id="menu_dish">
  <div class="row menu-item" id="{{- key }}">
    <div class="col-d-12">
      <div class="pull-left col-md-11">
        <h1>{{- name }}</h1>
        <ul class="details">
          {{ _.forIn(attributes, function(value, key) { }}
            {{ if (value === true) print('<li>' + key + '</li>') }}
          {{ }); }}
        <ul>
      </div>
      <div class="pull-right col-md-1 like-count">
        {{ if (rights === 'admin') { }}
          <a class="delete-item" style="cursor: pointer;">
            &#10008;
          </a>
        {{ } else if (rights === 'display') { }}
          <span class="glyphicon glyphicon-heart"></span>
          <h1>{{- count }}</h1>
        {{ } else { }}
          <a href="#">
            <span class="like-button glyphicon glyphicon-heart" style="{{ if(liked === true) print('color: #38b800') }}"></span>
          </a>
        {{ } }}
      </div>
    </div>
  </div>
</script>